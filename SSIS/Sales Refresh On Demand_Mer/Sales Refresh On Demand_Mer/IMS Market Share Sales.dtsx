                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select   ISNULL(s.FK_DosageFormID,-1) as 'FK_DosageFormID',s.FK_IMSDosageFormId,
ISNULL(x.FK_GeographicalAreaID,s.FK_CountryID*-1-100) as 'FK_GeographicalAreaID',
isnull(s.FK_IMSBrickId,-1) FK_IMSBrickId,
isnull(s.FK_IMSFormID,-1) FK_IMSFormID
,isnull(s.FK_IMSTherapeuticID,-1) FK_IMSTherapeuticID
,isnull(FK_IMSClientMarketId,-1) FK_IMSClientMarketId,
isnull(s.FK_IMSCorporationId,-1) FK_IMSCorporationId,
isnull(FK_ManufacturerId,-1) FK_ManufacturerId
,isnull(FK_IMSTerritoryId,-1) FK_IMSTerritoryId,CountingUnits,DosageUnits,
s.IMSPeriodCode,s.FK_PeriodID,
s.FK_CountryID,
(s.qty/ISNULL((select (Case When count(*) = 0 Then null else COUNT(*) END)  as a from IMSGeographicalAreaMapping g Where g.IMSSubTerritoryCode =(select IMSBrickCode from IMSBrick where IMSBrick.IMSBrickID = s.FK_IMSBrickId )  and s.FK_CountryID = g.FK_CountryID
),1)) as 'Qty',
(s.amount/ISNULL((select (Case When count(*) = 0 Then null else COUNT(*) END)  as a from IMSGeographicalAreaMapping g
where  g.IMSSubTerritoryCode = (select IMSBrickCode from IMSBrick where IMSBrick.IMSBrickID = s.FK_IMSBrickId )   and s.FK_CountryID = g.FK_CountryID
),1))  as 'Amount'
from IMSSales s
Left join  IMSGeographicalAreaMapping x on x.IMSSubTerritoryCode = (select IMSBrickCode from IMSBrick where IMSBrick.IMSBrickID = s.FK_IMSBrickId )   and s.FK_CountryID = x.FK_CountryID</property>
