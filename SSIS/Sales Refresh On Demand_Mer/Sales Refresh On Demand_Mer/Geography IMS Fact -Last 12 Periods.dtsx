                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  FK_PeriodID,  case when FK_SubDistributorGroupID is null then isnull(s.FK_CustomerBranchID, -2) else isnull(s.FK_CustomerBranchID, -1) end as FK_CustomerBranchID, ISNULL(s.FK_GeographicalAreaId, isnull(CustomerBranch.FK_GeographicalAreaID, s.FK_CountryID*-1-100)) AS FK_GeographicalAreaID, isnull(SUM(Qty),0) AS Qty, 
                      isnull(SUM(Amount),0) AS Amount,CASE WHEN s.FK_DistributorID is null and s.FK_SubDistributorGroupID is null THEN -1 WHEN s.FK_DistributorID is null and s.FK_SubDistributorGroupID is not null THEN -2 ELSE s.FK_DistributorID END as FK_DistributorID,   FK_DosageFormID,
case when  CustomerGroupType = 2 and FK_SubDistributorGroupID is null then 'AC' 
When FK_SubDistributorGroupID is not null then 'D' 
when CustomerGroupType = 1 then 'E' else 'N' END AS SalesType
, ISNULL(FK_GlobalDosageFormId,-1) AS 'FK_GlobalDosageFormId',
ISNULL(FK_DistributorBranchID,-1) AS 'FK_DistributorBranchID',
ISNULL(FK_DistributorCustomerMappingID,-1) FK_DistributorCustomerMappingID,
ISNULL(FK_DistributorProductMappingID,-1)FK_DistributorProductMappingID

FROM         sales s with(nolock) LEFT OUTER JOIN 
             CustomerBranch ON s.FK_CustomerBranchID = CustomerBranch.CustomerBranchID AND  
             CustomerBranch.FK_FromPeriodID &lt;= s.FK_PeriodID AND (CustomerBranch.FK_ToPeriodID IS NULL OR 
             CustomerBranch.FK_ToPeriodID &gt;= s.FK_PeriodID) LEFT OUTER JOIN 
             CustomerGroup ON CustomerGroup.CustomerGroupID = CustomerBranch.FK_CustomerGroupID /*AND  
             CustomerGroup.FK_FromPeriodID &lt;= s.FK_PeriodID AND (CustomerGroup.FK_ToPeriodID IS NULL OR 
             CustomerGroup.FK_ToPeriodID &gt;= s.FK_PeriodID) */
left outer join DosageForm on DosageFormId = s.FK_DosageFormID
                      left outer join GlobalDosageForm on GlobalDosageFormId = FK_GlobalDosageFormId
WHERE    (TransactionType IN ('R', 'S')) and (CustomerGroupType &lt;&gt; 0 or CustomerGroupType is null) 
AND (s.FK_PeriodID &gt;= ?) AND (s.FK_PeriodID &lt;= ?) AND (s.FK_CountryID IN (?))
GROUP BY FK_PeriodID,  ISNULL(s.FK_GeographicalAreaId, isnull(CustomerBranch.FK_GeographicalAreaID, s.FK_CountryID*-1-100)) , FK_DistributorID, 
                    case when FK_SubDistributorGroupID is null then isnull(s.FK_CustomerBranchID, -2) else isnull(s.FK_CustomerBranchID, -1) end,   FK_SubDistributorGroupID, FK_DosageFormID, case when  CustomerGroupType = 2 and FK_SubDistributorGroupID is null then 'AC' 
When FK_SubDistributorGroupID is not null then 'D' 
when CustomerGroupType = 1 then 'E' else 'N' END,
ISNULL(FK_GlobalDosageFormId,-1),
ISNULL(FK_DistributorBranchID,-1) ,
ISNULL(FK_DistributorCustomerMappingID,-1) ,
ISNULL(FK_DistributorProductMappingID,-1)</property>
