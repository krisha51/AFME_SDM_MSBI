      DTS:DTSID="{C702A524-98F5-4E3A-B040-94BD40AC7536}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT  s.FK_PeriodID,  case when FK_SubDistributorGroupID is null then isnull(s.FK_CustomerBranchID, -2) else isnull(s.FK_CustomerBranchID, -1) end as FK_CustomerBranchID, ISNULL(s.FK_GeographicalAreaId, isnull(CustomerBranch.FK_GeographicalAreaID, s.FK_CountryID*-1-100)) AS FK_GeographicalAreaID, isnull(SUM(Qty),0) AS Qty, &#xA;                      isnull(SUM(Amount),0) AS Amount,CASE WHEN s.FK_DistributorID is null and s.FK_SubDistributorGroupID is null THEN -1 WHEN s.FK_DistributorID is null and s.FK_SubDistributorGroupID is not null THEN -2 ELSE s.FK_DistributorID END as FK_DistributorID,   FK_DosageFormID,&#xA;case when  CustomerGroupType = 2 and FK_SubDistributorGroupID is null then 'AC' &#xA;When FK_SubDistributorGroupID is not null then 'D' &#xA;when CustomerGroupType = 1 then 'E' else 'N' END AS SalesType&#xA;, ISNULL(FK_GlobalDosageFormId,-1) AS 'FK_GlobalDosageFormId',&#xA;ISNULL(FK_DistributorBranchID,-1) AS 'FK_DistributorBranchID',&#xA;ISNULL(FK_DistributorCustomerMappingID,-1) FK_DistributorCustomerMappingID,&#xA;ISNULL(FK_DistributorProductMappingID,-1)FK_DistributorProductMappingID,s.fileamount, &#xA;CASE WHEN isdate(SalesDate) = 1 THEN DATEADD(dd, DATEDIFF(dd, 0, SalesDate), 0) ELSE DATEADD(dd, DATEDIFF(dd, 0, CountryPeriod.DateTo), 0) END as SalesDate  ,CASE WHEN s.TransactionType = 'S' THEN 1&#xA;WHEN s.TransactionType = 'R' THEN 2&#xA;WHEN s.TransactionType = 'B' THEN 3&#xA;WHEN s.TransactionType = 'RB' THEN 4&#xA;WHEN s.TransactionType = 'E' THEN 5 &#xA;ELSE 1 END AS 'FK_TransactionTypeID' &#xA;&#xA;&#xA;FROM         sales s with(nolock) INNER JOIN &#xA;             CountryPeriod ON CountryPeriod.FK_PeriodID = s.FK_PeriodID AND CountryPeriod.FK_CountryID = s.FK_CountryID  LEFT OUTER JOIN &#xA;             CustomerBranch ON s.FK_CustomerBranchID = CustomerBranch.CustomerBranchID AND  &#xA;             CustomerBranch.FK_FromPeriodID &lt;= s.FK_PeriodID AND (CustomerBranch.FK_ToPeriodID IS NULL OR &#xA;             CustomerBranch.FK_ToPeriodID &gt;= s.FK_PeriodID) LEFT OUTER JOIN &#xA;             CustomerGroup ON CustomerGroup.CustomerGroupID = CustomerBranch.FK_CustomerGroupID /*AND  &#xA;             CustomerGroup.FK_FromPeriodID &lt;= s.FK_PeriodID AND (CustomerGroup.FK_ToPeriodID IS NULL OR &#xA;             CustomerGroup.FK_ToPeriodID &gt;= s.FK_PeriodID) */&#xA;left outer join DosageForm on DosageFormId = s.FK_DosageFormID&#xA;                      left outer join GlobalDosageForm on GlobalDosageFormId = FK_GlobalDosageFormId&#xA;WHERE  (CustomerGroupType &lt;&gt; 0 or CustomerGroupType is null) &#xA;AND (s.FK_PeriodID &gt;= &quot; + (DT_WSTR,4) @[User::FromPeriodID]  + &quot;) AND (s.FK_PeriodID &lt;= &quot; + (DT_WSTR,4) @[User::ToPeriodID]  + &quot;) AND (s.FK_CountryID IN (&quot; + @[$Project::_MarketCountries]  + &quot;))&#xA; GROUP BY s.FK_PeriodID,  ISNULL(s.FK_GeographicalAreaId, isnull(CustomerBranch.FK_GeographicalAreaID, s.FK_CountryID*-1-100)) , FK_DistributorID, &#xA;                    case when FK_SubDistributorGroupID is null then isnull(s.FK_CustomerBranchID, -2) else isnull(s.FK_CustomerBranchID, -1) end,   FK_SubDistributorGroupID, FK_DosageFormID, case when  CustomerGroupType = 2 and FK_SubDistributorGroupID is null then 'AC' &#xA;When FK_SubDistributorGroupID is not null then 'D' &#xA;when CustomerGroupType = 1 then 'E' else 'N' END,&#xA;ISNULL(FK_GlobalDosageFormId,-1),&#xA;ISNULL(FK_DistributorBranchID,-1) ,&#xA;ISNULL(FK_DistributorCustomerMappingID,-1) ,&#xA;ISNULL(FK_DistributorProductMappingID,-1),s.fileamount, CASE WHEN isdate(SalesDate) = 1 THEN DATEADD(dd, DATEDIFF(dd, 0, SalesDate), 0) ELSE DATEADD(dd, DATEDIFF(dd, 0, CountryPeriod.DateTo), 0) END  ,s.TransactionType &quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SqlFillGeo">
      <DTS:VariableValue
        DTS:DataType="8">SELECT  s.FK_PeriodID,  case when FK_SubDistributorGroupID is null then isnull(s.FK_CustomerBranchID, -2) else isnull(s.FK_CustomerBranchID, -1) end as FK_CustomerBranchID, ISNULL(s.FK_GeographicalAreaId, isnull(CustomerBranch.FK_GeographicalAreaID, s.FK_CountryID*-1-100)) AS FK_GeographicalAreaID, isnull(SUM(Qty),0) AS Qty, 
                      isnull(SUM(Amount),0) AS Amount,CASE WHEN s.FK_DistributorID is null and s.FK_SubDistributorGroupID is null THEN -1 WHEN s.FK_DistributorID is null and s.FK_SubDistributorGroupID is not null THEN -2 ELSE s.FK_DistributorID END as FK_DistributorID,   FK_DosageFormID,
case when  CustomerGroupType = 2 and FK_SubDistributorGroupID is null then 'AC' 
When FK_SubDistributorGroupID is not null then 'D' 
when CustomerGroupType = 1 then 'E' else 'N' END AS SalesType
, ISNULL(FK_GlobalDosageFormId,-1) AS 'FK_GlobalDosageFormId',
ISNULL(FK_DistributorBranchID,-1) AS 'FK_DistributorBranchID',
ISNULL(FK_DistributorCustomerMappingID,-1) FK_DistributorCustomerMappingID,
ISNULL(FK_DistributorProductMappingID,-1)FK_DistributorProductMappingID,s.fileamount, 
CASE WHEN isdate(SalesDate) = 1 THEN DATEADD(dd, DATEDIFF(dd, 0, SalesDate), 0) ELSE DATEADD(dd, DATEDIFF(dd, 0, CountryPeriod.DateTo), 0) END as SalesDate  ,CASE WHEN s.TransactionType = 'S' THEN 1
WHEN s.TransactionType = 'R' THEN 2
WHEN s.TransactionType = 'B' THEN 3
WHEN s.TransactionType = 'RB' THEN 4
WHEN s.TransactionType = 'E' THEN 5 
ELSE 1 END AS 'FK_TransactionTypeID' 


FROM         sales s with(nolock) INNER JOIN 
             CountryPeriod ON CountryPeriod.FK_PeriodID = s.FK_PeriodID AND CountryPeriod.FK_CountryID = s.FK_CountryID  LEFT OUTER JOIN 
             CustomerBranch ON s.FK_CustomerBranchID = CustomerBranch.CustomerBranchID AND  
             CustomerBranch.FK_FromPeriodID &lt;= s.FK_PeriodID AND (CustomerBranch.FK_ToPeriodID IS NULL OR 
             CustomerBranch.FK_ToPeriodID &gt;= s.FK_PeriodID) LEFT OUTER JOIN 
             CustomerGroup ON CustomerGroup.CustomerGroupID = CustomerBranch.FK_CustomerGroupID /*AND  
             CustomerGroup.FK_FromPeriodID &lt;= s.FK_PeriodID AND (CustomerGroup.FK_ToPeriodID IS NULL OR 
             CustomerGroup.FK_ToPeriodID &gt;= s.FK_PeriodID) */
left outer join DosageForm on DosageFormId = s.FK_DosageFormID
                      left outer join GlobalDosageForm on GlobalDosageFormId = FK_GlobalDosageFormId
WHERE  (CustomerGroupType &lt;&gt; 0 or CustomerGroupType is null) 
AND (s.FK_PeriodID &gt;= 0) AND (s.FK_PeriodID &lt;= 242) AND (s.FK_CountryID IN (6))
 GROUP BY s.FK_PeriodID,  ISNULL(s.FK_GeographicalAreaId, isnull(CustomerBranch.FK_GeographicalAreaID, s.FK_CountryID*-1-100)) , FK_DistributorID, 
                    case when FK_SubDistributorGroupID is null then isnull(s.FK_CustomerBranchID, -2) else isnull(s.FK_CustomerBranchID, -1) end,   FK_SubDistributorGroupID, FK_DosageFormID, case when  CustomerGroupType = 2 and FK_SubDistributorGroupID is null then 'AC' 
When FK_SubDistributorGroupID is not null then 'D' 
when CustomerGroupType = 1 then 'E' else 'N' END,
ISNULL(FK_GlobalDosageFormId,-1),
ISNULL(FK_DistributorBranchID,-1) ,
ISNULL(FK_DistributorCustomerMappingID,-1) ,
ISNULL(FK_DistributorProductMappingID,-1),s.fileamount, CASE WHEN isdate(SalesDate) = 1 THEN DATEADD(dd, DATEDIFF(dd, 0, SalesDate), 0) ELSE DATEADD(dd, DATEDIFF(dd, 0, CountryPeriod.DateTo), 0) END  ,s.TransactionType </DTS:VariableValue>
    </DTS:Variable>
